{
  "extends": [ "prettier", "prettier/flowtype", "prettier/react", "prettier/standard", "plugin:react/recommended"],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {},
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {},
  "rules": {
    "react/jsx-uses-vars": 1,
    "no-unused-vars": "error",
    "no-debugger": "error",
    "no-console": "error"
  }
}
